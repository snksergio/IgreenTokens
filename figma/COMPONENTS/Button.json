{
  "igreen": {
    "button": {
      "bg": {
        "enabled": {
          "default": {
            "$type": "color",
            "$value": "{igreen.scheme.bg.primary}"
          }
        }
      },
      "base": {
        "height": {
          "xs": {
            "$type": "sizing",
            "$value": "{igreen.forms.base.height.xs}"
          },
          "sm": {
            "$type": "sizing",
            "$value": "{igreen.forms.base.height.sm}"
          },
          "md": {
            "$type": "sizing",
            "$value": "{igreen.forms.base.height.md}"
          },
          "lg": {
            "$type": "sizing",
            "$value": "{igreen.forms.base.height.lg}"
          },
          "xl": {
            "$type": "sizing",
            "$value": "{igreen.forms.base.height.xl}"
          }
        }
      }
    }
  }
}