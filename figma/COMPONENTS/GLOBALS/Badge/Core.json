{
  "igreen": {
    "badge": {
      "base": {
        "size": {
          "sm": {
            "$type": "sizing",
            "$value": "{igreen.dimension.1_5}"
          },
          "md": {
            "$type": "sizing",
            "$value": "{igreen.size.xs4}"
          },
          "lg": {
            "$type": "sizing",
            "$value": "{igreen.size.xs3}"
          }
        },
        "count": {
          "min-width": {
            "$type": "sizing",
            "$value": "{igreen.size.xs}"
          },
          "height": {
            "$type": "sizing",
            "$value": "{igreen.size.xs}"
          },
          "padding-horizontal": {
            "$type": "spacing",
            "$value": "{igreen.dimension.1_5}"
          }
        },
        "bg": {
          "critical": {
            "$type": "color",
            "$value": "{igreen.bg.critical.main.enabled.default}"
          },
          "success": {
            "$type": "color",
            "$value": "{igreen.bg.success.main.enabled.default}"
          },
          "warning": {
            "$type": "color",
            "$value": "{igreen.bg.warning.main.enabled.default}"
          },
          "info": {
            "$type": "color",
            "$value": "{igreen.bg.info.main.enabled.default}"
          }
        }
      },
      "Typography": {
        "size": {
          "$type": "text",
          "$value": "sm"
        },
        "variant": {
          "$type": "text",
          "$value": "body"
        },
        "color": {
          "$type": "color",
          "$value": "{igreen.color.white}"
        }
      },
      "ring": {
        "opacity": {
          "default": {
            "$type": "opacity",
            "$value": "0.1"
          },
          "solid": {
            "$type": "opacity",
            "$value": "1"
          }
        },
        "padding": {
          "$type": "spacing",
          "$value": "{igreen.size.xs5}"
        }
      },
      "border-radius": {
        "$type": "borderRadius",
        "$value": "{igreen.radii.full}"
      }
    }
  }
}