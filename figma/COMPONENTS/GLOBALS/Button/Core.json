{
  "igreen": {
    "button": {
      "base": {
        "height": {
          "sm": {
            "$type": "sizing",
            "$value": "{igreen.form.base.height.sm}"
          },
          "md": {
            "$type": "sizing",
            "$value": "{igreen.form.base.height.md}"
          },
          "lg": {
            "$type": "sizing",
            "$value": "{igreen.form.base.height.lg}"
          },
          "xl": {
            "$type": "sizing",
            "$value": "{igreen.form.base.height.xl}"
          }
        },
        "gap": {
          "xl": {
            "$type": "spacing",
            "$value": "{igreen.gap.xs}"
          },
          "lg": {
            "$type": "spacing",
            "$value": "{igreen.gap.xs}"
          },
          "md": {
            "$type": "spacing",
            "$value": "{igreen.gap.xs2}"
          },
          "sm": {
            "$type": "spacing",
            "$value": "{igreen.gap.xs2}"
          }
        },
        "horizontals": {
          "padding": {
            "xl": {
              "$type": "spacing",
              "$value": "{igreen.padding.xl2}"
            },
            "lg": {
              "$type": "spacing",
              "$value": "{igreen.padding.xl}"
            },
            "md": {
              "$type": "spacing",
              "$value": "{igreen.padding.lg}"
            },
            "sm": {
              "$type": "spacing",
              "$value": "{igreen.padding.md}"
            }
          }
        },
        "borderRadius": {
          "pill": {
            "$type": "borderRadius",
            "$value": "{igreen.radii.full}"
          }
        },
        "outline": {
          "border": {
            "primary": {
              "enabled": {
                "$type": "border",
                "$value": {
                  "color": "{igreen.border-color.primary}",
                  "width": "{igreen.border-width.thin}",
                  "style": "solid"
                }
              }
            },
            "neutral": {
              "enabled": {
                "$type": "border",
                "$value": {
                  "color": "{igreen.border-color.moderate}",
                  "width": "{igreen.border-width.thin}",
                  "style": "solid"
                }
              }
            },
            "seconday": {
              "enabled": {
                "$type": "border",
                "$value": {
                  "color": "{igreen.border-color.seconday}",
                  "width": "{igreen.border-width.thin}",
                  "style": "solid"
                }
              }
            },
            "warning": {
              "enabled": {
                "$type": "border",
                "$value": {
                  "color": "{igreen.border-color.warning}",
                  "width": "{igreen.border-width.thin}",
                  "style": "solid"
                }
              }
            },
            "disabled": {
              "$type": "border",
              "$value": {
                "color": "{igreen.border-color.disabled}",
                "width": "{igreen.border-width.thin}",
                "style": "solid"
              }
            }
          }
        },
        "borderWidth": {
          "$type": "borderWidth",
          "$value": "{igreen.border-width.thin}"
        }
      },
      "icon": {
        "size": {
          "lg": {
            "$type": "sizing",
            "$value": "{igreen.size.xs}"
          },
          "md": {
            "$type": "sizing",
            "$value": "{igreen.size.xs2}"
          },
          "sm": {
            "$type": "sizing",
            "$value": "{igreen.size.xs2}"
          }
        }
      }
    }
  }
}