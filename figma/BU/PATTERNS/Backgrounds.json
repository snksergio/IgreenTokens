{
  "igreen": {
    "regular": {
      "bg": {
        "primary": {
          "enabled": {
            "value": "{igreen.scheme.bg.primary}",
            "type": "color"
          },
          "hover": {
            "$extensions": {
              "studio.tokens": {
                "modify": {
                  "type": "darken",
                  "value": "{igreen.state.mod.hover} ",
                  "space": "lch"
                }
              }
            },
            "value": "{igreen.scheme.bg.primary}",
            "type": "color"
          }
        },
        "surface-main": {
          "hover": {
            "value": "{igreen.scheme.bg.surface-main}",
            "type": "color",
            "$extensions": {
              "studio.tokens": {
                "modify": {
                  "type": "lighten",
                  "value": "{igreen.scheme.mod-amount.1}",
                  "space": "lch"
                }
              }
            }
          },
          "enabled": {
            "value": "{igreen.scheme.bg.surface-main}",
            "type": "color"
          }
        }
      }
    }
  }
}