{
  "igreen": {
    "regular": {
      "bg": {
        "primary": {
          "enabled": {
            "$type": "color",
            "$value": "{igreen.scheme.bg.primary}"
          },
          "hover": {
            "$extensions": {
              "studio.tokens": {
                "modify": {
                  "type": "darken",
                  "value": "{igreen.state.mod.hover} ",
                  "space": "lch"
                }
              }
            },
            "$type": "color",
            "$value": "{igreen.scheme.bg.primary}"
          }
        },
        "surface-main": {
          "enabled": {
            "$type": "color",
            "$value": "{igreen.scheme.bg.surface-main}"
          },
          "hover": {
            "$extensions": {
              "studio.tokens": {
                "modify": {
                  "type": "darken",
                  "value": "{igreen.scheme.mod-amount.1}",
                  "space": "lch"
                }
              }
            },
            "$type": "color",
            "$value": "{igreen.scheme.bg.surface-main}"
          }
        }
      }
    }
  }
}